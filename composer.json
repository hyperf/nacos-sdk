{
    "name": "hyperf/nacos-sdk",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "nacos"
    ],
    "description": "Nacos SDK",
    "autoload": {
        "psr-4": {
            "Hyperf\\NacosSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\NacosSdk\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "hyperf/contract": "~2.2.0",
        "hyperf/utils": "~2.2.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "hyperf": {
            "config": "Hyperf\\NacosSdk\\ConfigProvider"
        }
    }
}
